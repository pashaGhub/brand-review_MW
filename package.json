{
  "name": "brand-page_mw",
  "version": "1.0.0",
  "description": "MERN",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix front",
    "client:install": "npm install --prefix front",
    "client:build": "npm run build --prefix front",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "MERN",
    "React",
    "Node",
    "Express",
    "MongoDB"
  ],
  "author": "Pavel Sucharev",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.1",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "@types/react-scroll": "^1.8.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "fs": "0.0.1-security",
    "nodemon": "^2.0.4",
    "react-scroll": "^1.8.1"
  }
}
